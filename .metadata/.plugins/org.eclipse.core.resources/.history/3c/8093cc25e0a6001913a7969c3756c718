package zoo;
import org.animals.*;

public class VandalurZoo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Lion l = new Lion();
		
		System.out.println(l.color);
		System.out.println(l.weight);
		
		boolean b = l.canClimb();
		System.out.println("Lion can climb?: "+ b);
		boolean v = l.isVegetarian();
		System.out.println("Lion is vegetarian?: "+ v);
		String sn = l.getSound();
		System.out.println("Lion sound?: "+ sn);
	
		
		Elephant e = new Elephant();
		boolean be = e.canClimb();
		System.out.println("Elephant can climb: "+ be);
		boolean ve = e.isVegetarian();
		System.out.println("Elephant is vegetarian: "+ ve);
		String sne = e.getSound();
		System.out.println("Elephant sound: "+ sne);
		
		Monkey m = new Monkey();
		boolean bm = m.canClimb();
		System.out.println("Monkey can climb?: "+ bm);
		boolean vm = m.isVegetarian();
		System.out.println("Monkey is vegetarian?: "+ vm);
		String snm = m.getSound();
		System.out.println("Monkey sound: "+ snm);
		
		
		
		
	}

}
