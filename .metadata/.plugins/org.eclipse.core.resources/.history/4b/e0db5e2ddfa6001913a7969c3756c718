package org.animals;


public class Lion {
	String color ="orange";
	double weight = 60;
	int age = 5;
	public boolean isVegetarian()
	{	return false;
		
	}
	public boolean canClimb()
	{	return false;
		
	}
	public String getSound()
	{	return "roar";
		
	}

}

 class Monkey{
	String color ="Brown";
	double weight = 60;
	int age = 5;
	public boolean isVegetarian()
	{	return false;
		
	}
	public boolean canClimb()
	{	return true;
		
	}
	public String getSound()
	{	return "roar";
		
	}
	
}
 
 
 class Elephant{
		String color ="Grey";
		double weight = 260;
		int age = 5;
		public boolean isVegetarian()
		{	return false;
			
		}
		public boolean canClimb()
		{	return false;
			
		}
		public String getSound()
		{	return "roar";
			
		}
		
	}

