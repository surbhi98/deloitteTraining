package zoo;
import org.animals.*;

public class VandalurZoo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Lion l = new Lion();
		
		
		
		boolean b = l.canClimb();
		System.out.println("Lion can climb?: "+ b);
		boolean v = l.isVegetarian();
		System.out.println("Lion is vegetarian?: "+ v);
		String sn = l.getSound();
		System.out.println("Lion sound?: "+ sn);
		
		
		
		
		
	}

}
